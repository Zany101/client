<div class="tab-pane fade" id="{{ template(_self)  }}" role="tabpanel" aria-labelledby="{{ template(_self)  }}-tab">
  <div class="form-row">
      <div class="form-group col">
          {{ form_widget(form.gamePaths.relativeExecutable) }}
          <div class="form-error">{{ form_errors(form.gamePaths.relativeExecutable) }}</div>
