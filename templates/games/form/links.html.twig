
{% block navTab %}
  <li class="nav-item" role="presentation">
    <button class="nav-link" id="{{ template(_self) }}-tab" data-toggle="tab" data-target="#{{ template(_self) }}" type="button" role="tab" aria-controls="{{ template(_self) }}" aria-selected="false">{{ template(_self)|capitalize }}</button>
  </li>
{% endblock %}
{% block tab %}
<div class="tab-pane fade" id="{{ template(_self)  }}" role="tabpanel" aria-labelledby="{{ template(_self)  }}-tab">
  <div class="form-row">
      <div class="form-group col">
          {{ form_widget(form.gameLinks.linkId) }}
          <div class="form-error">{{ form_errors(form.gameLinks.linkId) }}</div>
      </div>
  </div>
  <div class="form-row">
      <div class="form-group col">
          {{ form_widget(form.gameLinks.modId) }}
          <div class="form-error">{{ form_errors(form.gameLinks.modId) }}</div>
      </div>
  </div>
  <div class="form-row">
      <div class="form-group col">
          {{ form_widget(form.gameLinks.url) }}
          <div class="form-error">{{ form_errors(form.gameLinks.url) }}</div>
      </div>
  </div>
  <div class="form-row">
      <div class="form-group col">
          {{ form_widget(form.gameLinks.displayName) }}
          <div class="form-error">{{ form_errors(form.gameLinks.displayName) }}</div>
      </div>
  </div>
  <div class="form-row">
      <div class="form-group col">
          {{ form_widget(form.gameLinks.icon) }}
          <div class="form-error">{{ form_errors(form.gameLinks.icon) }}</div>
      </div>
  </div>
  <div class="form-row">
      <div class="form-group col">
          {{ form_widget(form.gameLinks.description) }}
          <div class="form-error">{{ form_errors(form.gameLinks.description) }}</div>
      </div>
  </div>
  <div class="form-row">
      <div class="form-group col">
          {{ form_widget(form.gameLinks.newPage) }}
          <div class="form-error">{{ form_errors(form.gameLinks.newPage) }}</div>
      </div>
  </div>
  <div class="form-row">
      <div class="form-group col">
          {{ form_widget(form.gameLinks.insidePanel) }}
          <div class="form-error">{{ form_errors(form.gameLinks.insidePanel) }}</div>
      </div>
  </div>
  <div class="form-row">
      <div class="form-group col">
          {{ form_widget(form.gameLinks.categoryId) }}
          <div class="form-error">{{ form_errors(form.gameLinks.categoryId) }}</div>
      </div>
  </div>
</div>
{% endblock %}
