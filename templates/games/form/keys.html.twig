
{% block navTab %}
  <li class="nav-item" role="presentation">
    <button class="nav-link" id="{{ template(_self) }}-tab" data-toggle="tab" data-target="#{{ template(_self) }}" type="button" role="tab" aria-controls="{{ template(_self) }}" aria-selected="false">{{ template(_self)|capitalize }}</button>
  </li>
{% endblock %}
{% block tab %}
<div class="tab-pane fade" id="{{ template(_self)  }}" role="tabpanel" aria-labelledby="{{ template(_self)  }}-tab">
  <div class="form-row">
      <div class="form-group col">
          {{ form_widget(form.gameKeys.keyId) }}
          <div class="form-error">{{ form_errors(form.gameKeys.keyId) }}</div>
      </div>
  </div>
  <div class="form-row">
      <div class="form-group col">
          {{ form_widget(form.gameKeys.gameKey) }}
          <div class="form-error">{{ form_errors(form.gameKeys.gameKey) }}</div>
      </div>
  </div>
  <div class="form-row">
      <div class="form-group col">
          {{ form_widget(form.gameKeys.serverId) }}
          <div class="form-error">{{ form_errors(form.gameKeys.serverId) }}</div>
      </div>
  </div>
  <div class="form-row">
      <div class="form-group col">
          {{ form_widget(form.gameKeys.serviceId) }}
          <div class="form-error">{{ form_errors(form.gameKeys.serviceId) }}</div>
      </div>
  </div>
  <div class="form-row">
      <div class="form-group col">
          {{ form_widget(form.gameKeys.reusable) }}
          <div class="form-error">{{ form_errors(form.gameKeys.reusable) }}</div>
      </div>
  </div>
  <div class="form-row">
      <div class="form-group col">
          {{ form_widget(form.gameKeys.ipAddress) }}
          <div class="form-error">{{ form_errors(form.gameKeys.ipAddress) }}</div>
      </div>
  </div>
</div>
{% endblock %}
